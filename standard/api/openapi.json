      "put": {
        "operationId": "upload",
        "parameters": [
          {
            "name": "blobId",
            "in": "path",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/uuid"
            }
          }
        ],
        "responses": {
          "200": {
